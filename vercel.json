{
  "version": 2,
  "name": "gateway-dex",
  "framework": null,
  "buildCommand": "yarn prepare && yarn build",
  "devCommand": "yarn start",
  "installCommand": "yarn install",
  "outputDirectory": "build",
  "trailingSlash": false,
  "public": false,
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=4096"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
